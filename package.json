{
  "name": "immutable-http",
  "version": "0.1.14",
  "description": "Http client with pretty simple chaining API",
  "keywords": [
    "javascript",
    "xhr",
    "http",
    "rest"
  ],
  "scripts": {
    "doc": "rm -rf docs && esdoc -c ./esdoc.json",
    "doc:publish": "gh-pages -d docs",
    "test": "karma start --single-run",
    "test:debug": "karma start",
    "test:babel": "babel test/get_test.js --out-file test/get_test_compiled.js",
    "clean": "rm -rf ./dist",
    "dist": "npm run clean && mkdir dist && webpack --progress --colors --config webpack.config.js",
    "prepublish": "npm run dist && npm run doc && npm run doc:publish"
  },
  "main": "dist/index.js",
  "homepage": "http://stremlenye.github.io/immutable-http/",
  "bugs": {
    "url": "https://github.com/stremlenye/immutable-http/issues"
  },
  "license": "MIT",
  "people": {
    "author": {
      "name": "Yuriy Ankudinov",
      "email": "stremlenye@gmail.com",
      "url": "https://github.com/stremlenye"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stremlenye/immutable-http.git"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.20",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.1",
    "body-parser": "^1.13.2",
    "chai": "^3.0.0",
    "cors": "^2.7.1",
    "esdoc": "^0.1.3",
    "eslint": "^0.24.1",
    "express": "^4.13.1",
    "gh-pages": "^0.3.1",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "webpack": "^1.10.1"
  },
  "dependencies": {
    "babel-runtime": "^5.6.20",
    "core-decorators": "^0.1.2"
  }
}

{
  "name": "immutable-http",
  "version": "0.1.8",
  "description": "Http client with pretty simple chaining API",
  "keywords": [
    "javascript",
    "xhr",
    "http",
    "rest"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./dist",
    "dist": "npm run clean && mkdir dist && babel --modules common src/immutable-http.js --out-file dist/immutable-http.js",
    "prepublish": "npm run dist"
  },
  "main": "dist/immutable-http.js",
  "homepage": "https://github.com/stremlenye/immutable-http",
  "bugs": {
    "url": "https://github.com/stremlenye/immutable-http/issues"
  },
  "license": "MIT",
  "people": {
    "author": {
      "name": "Yuriy Ankudinov",
      "email": "stremlenye@gmail.com",
      "url": "https://github.com/stremlenye"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stremlenye/immutable-http.git"
  },
  "dependencies": {
    "promise": "^7.0.3"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.23",
    "babel-jest": "^5.3.0",
    "body-parser": "^1.13.2",
    "eslint": "^0.24.1",
    "express": "^4.13.1",
    "jest": "^0.1.40",
    "jest-cli": "^0.4.15"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}
